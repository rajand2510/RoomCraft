{
  "version": 2,
  
    "rewrites": [
      { "source": "/(*)", "destination": "/" },
      { "source": "/(*)/$", "destination": "/$1" }
    ]
  ,
  "builds": [
    {
      "src": "./index.js",
      "use": "@vercel/node",
      "config": {
        "runtime": "nodejs16.x"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/"
    }
  ]
  
}
